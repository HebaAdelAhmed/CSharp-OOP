@{
}

@model List<Department>
<!--Strong Type View-->
<!--List Of Department-->

<table class="table table-bordered table-hover">
    <tr>
        <th>Department Name</th>
        <th>Manager Name</th>
    </tr>
    @foreach (var department in Model)
    {
        <tr>
            <td>@department.Name</td>
            <td>@department.ManagerName</td>
        </tr>
    }
</table>