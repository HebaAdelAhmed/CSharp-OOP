@{
}

@model List<Student>

<table class="table table-bordered table-hover">
    <tr>
        <th>Department Name</th>
        <th>Manager Name</th>
    </tr>
    @foreach (var student in Model)
    {
        <tr>
            <td>@student.Name</td>
            <td>@student.Age</td>
            <td>@student.Address</td>
            <td>@student.Image</td>
        </tr>
    }
</table>
