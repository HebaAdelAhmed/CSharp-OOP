@{
}

<table class="table table-bordered table-hover">

    @foreach (Student item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Address</td>
            <td><img src="/Imgs/@item.Image" style="width: 50px; height:50px" /></td>
        </tr>
    }
</table>